        "sirius": {
            "from_table": "cases",
            "transform": {
                "orderdate": "to_char({col}, 'YYYY-MM-DD')",
                "createddate": "to_char({col}, 'YYYY-MM-DD')",
                "orderissuedate": "to_char({col}, 'YYYY-MM-DD')",
                "receiptdate": "to_char({col}, 'YYYY-MM-DD')",
